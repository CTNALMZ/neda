      "Timeout": 60,
      "MaxRetries": 5,
      "SignatureValidation": {
        "Enabled": true,
        "Algorithm": "HMACSHA256",
        "Header": "X-NEDA-Signature"
      }
    },
    "MessageQueue": {
      "Enabled": true,
      "Provider": "RabbitMQ",
      "ConnectionString": "${RABBITMQ_CONNECTION_STRING}",
      "Exchanges": {
        "BuildEvents": "build.events",
        "DeploymentEvents": "deployment.events",
        "NotificationEvents": "notification.events"
      },
      "Queues": {
        "BuildQueue": "build.queue",
        "DeploymentQueue": "deployment.queue",
        "NotificationQueue": "notification.queue"
      }
