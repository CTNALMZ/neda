    <NaturalLanguageProcessing>
      <ModelPath>C:\Projects\NEDA\Models\NLP</ModelPath>
      <LanguageDetectionConfidence>0.7</LanguageDetectionConfidence>
      <EnableSentimentAnalysis>true</EnableSentimentAnalysis>
      <EnableEntityRecognition>true</EnableEntityRecognition>
      <MaxTextLength>10000</MaxTextLength>
      <Tokenizer>WhiteSpace</Tokenizer>
      <StopWords>
        <Language>en</Language>
        <Language>tr</Language>
      </StopWords>
    </NaturalLanguageProcessing>
    
    <ComputerVision>
      <ModelPath>C:\Projects\NEDA\Models\Vision</ModelPath>
      <ImageProcessingTimeout>30</ImageProcessingTimeout>
      <MaxImageSizeMB>10</MaxImageSizeMB>
      <SupportedFormats>
        <Format>jpg</Format>
