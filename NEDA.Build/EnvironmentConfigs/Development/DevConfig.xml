      <Endpoints>
        <Endpoint Path="/webhooks/github" Secret="github-webhook-secret" />
        <Endpoint Path="/webhooks/azure" Secret="azure-webhook-secret" />
        <Endpoint Path="/webhooks/slack" Secret="slack-webhook-secret" />
      </Endpoints>
    </Webhooks>
  </ExternalServices>

  <AIConfiguration>
    <MachineLearning>
      <ModelCachePath>C:\Projects\NEDA\Models\Cache</ModelCachePath>
      <TrainingDataPath>C:\Projects\NEDA\Data\Training</TrainingDataPath>
      <EnableGPU>false</EnableGPU>
      <MaxBatchSize>32</MaxBatchSize>
      <DefaultEpochs>50</DefaultEpochs>
      <ValidationSplit>0.2</ValidationSplit>
      <EarlyStoppingPatience>10</EarlyStoppingPatience>
      <LearningRate>0.001</LearningRate>
      <Optimizer>Adam</Optimizer>
      <LossFunction>CrossEntropy</LossFunction>
