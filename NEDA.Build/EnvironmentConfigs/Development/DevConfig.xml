<?xml version="1.0" encoding="utf-8"?>
<DevelopmentConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                          xsi:noNamespaceSchemaLocation="DevConfig.xsd"
                          Version="1.0"
                          LastUpdated="2024-01-15T10:30:00Z">
  
  <!-- NEDA Development Environment Configuration -->
  <!-- This file contains development-specific settings for the NEDA platform -->
  
  <EnvironmentInfo>
    <Name>NEDA Development Environment</Name>
    <Type>Development</Type>
    <Version>1.0.0-dev</Version>
    <Description>Local development environment for NEDA platform with full debugging capabilities</Description>
    <TargetFramework>net8.0</TargetFramework>
    <Platform>x64</Platform>
    <CompatibilityMode>Windows</CompatibilityMode>
  </EnvironmentInfo>

  <BuildConfiguration>
    <SolutionName>NEDA.sln</SolutionName>
    <SolutionPath>C:\Projects\NEDA\Source</SolutionPath>
    <OutputPath>C:\Projects\NEDA\Bin\Debug</OutputPath>
    <IntermediateOutputPath>C:\Projects\NEDA\Obj\Debug</IntermediateOutputPath>
    
    <Projects>
      <Project Name="NEDA.Core" Path="NEDA.Core\NEDA.Core.csproj" BuildOrder="1" />
      <Project Name="NEDA.UI" Path="NEDA.UI\NEDA.UI.csproj" BuildOrder="2" />
      <Project Name="NEDA.API" Path="NEDA.API\NEDA.API.csproj" BuildOrder="3" />
      <Project Name="NEDA.Services" Path="NEDA.Services\NEDA.Services.csproj" BuildOrder="4" />
      <Project Name="NEDA.Brain" Path="NEDA.Brain\NEDA.Brain.csproj" BuildOrder="5" />
      <Project Name="NEDA.Automation" Path="NEDA.Automation\NEDA.Automation.csproj" BuildOrder="6" />
    </Projects>
    
    <BuildSettings>
      <Configuration>Debug</Configuration>
      <Platform>AnyCPU</Platform>
      <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
      <Optimize>false</Optimize>
      <DebugType>full</DebugType>
      <DebugSymbols>true</DebugSymbols>
      <DefineConstants>DEBUG;TRACE;DEVELOPMENT</DefineConstants>
      <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
      <Nullable>enable</Nullable>
      <LangVersion>latest</LangVersion>
      <NoWarn>1701;1702;1591</NoWarn>
      <WarningsAsErrors />
    </BuildSettings>
    
    <CodeAnalysis>
      <Enabled>true</Enabled>
      <Ruleset>C:\Projects\NEDA\Config\CodeAnalysis\Development.ruleset</Ruleset>
      <Analyzers>
        <Analyzer Assembly="Microsoft.CodeAnalysis.CSharp" />
        <Analyzer Assembly="StyleCop.Analyzers" />
        <Analyzer Assembly="SonarAnalyzer.CSharp" />
      </Analyzers>
    </CodeAnalysis>
    
    <Testing>
      <TestFramework>xUnit</TestFramework>
      <TestRunner>vstest</TestRunner>
      <ParallelExecution>true</ParallelExecution>
      <MaxParallelThreads>4</MaxParallelThreads>
      <CodeCoverageEnabled>true</CodeCoverageEnabled>
      <CoverageTool>coverlet</CoverageTool>
      <CoverageThreshold>70</CoverageThreshold>
      <TestResultsPath>C:\Projects\NEDA\TestResults</TestResultsPath>
    </Testing>
    
    <Packaging>
      <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
      <PackageOutputPath>C:\Projects\NEDA\Packages</PackageOutputPath>
      <IncludeSymbols>true</IncludeSymbols>
      <SymbolPackageFormat>snupkg</SymbolPackageFormat>
      <PackageVersion>1.0.0-dev</PackageVersion>
    </Packaging>
  </BuildConfiguration>

  <RuntimeConfiguration>
    <ExecutionEnvironment>
      <DotNetRuntime>
        <Version>8.0.0</Version>
        <RollForward>LatestMinor</RollForward>
        <SelfContained>false</SelfContained>
        <RuntimeIdentifier>win-x64</RuntimeIdentifier>
        <UseAppHost>true</UseAppHost>
      </DotNetRuntime>
      
      <EnvironmentVariables>
        <Variable Name="ASPNETCORE_ENVIRONMENT" Value="Development" />
        <Variable Name="DOTNET_ENVIRONMENT" Value="Development" />
        <Variable Name="ASPNETCORE_URLS" Value="https://localhost:5001;http://localhost:5000" />
        <Variable Name="DOTNET_CLI_TELEMETRY_OPTOUT" Value="1" />
        <Variable Name="DOTNET_SKIP_FIRST_TIME_EXPERIENCE" Value="1" />
        <Variable Name="DOTNET_NOLOGO" Value="1" />
        <Variable Name="NEDA_DEVELOPMENT_MODE" Value="true" />
      </EnvironmentVariables>
      
      <GarbageCollection>
        <ServerGC>true</ServerGC>
        <ConcurrentGC>true</ConcurrentGC>
        <RetainVM>false</RetainVM>
        <LatencyMode>Interactive</LatencyMode>
      </GarbageCollection>
      
      <ThreadPool>
        <MinThreads>10</MinThreads>
        <MaxThreads>32767</MaxThreads>
        <CompletionPortThreads Enabled="true" Min="10" Max="1000" />
      </ThreadPool>
    </ExecutionEnvironment>
    
    <Hosting>
      <Kestrel>
        <Endpoints>
          <Endpoint Name="Http" Url="http://localhost:5000" Protocol="Http" />
          <Endpoint Name="Https" Url="https://localhost:5001" Protocol="Https">
            <Certificate Path="C:\Projects\NEDA\Certificates\localhost.pfx" Password="dev-cert-password" />
          </Endpoint>
          <Endpoint Name="Grpc" Url="http://localhost:5002" Protocol="Http2" />
        </Endpoints>
        
        <Limits>
          <MaxRequestBodySize>10485760</MaxRequestBodySize>
          <MaxRequestBufferSize>1048576</MaxRequestBufferSize>
          <MaxRequestLineSize>8192</MaxRequestLineSize>
          <MaxRequestHeadersTotalSize>32768</MaxRequestHeadersTotalSize>
          <KeepAliveTimeout>120</KeepAliveTimeout>
          <RequestHeadersTimeout>30</RequestHeadersTimeout>
        </Limits>
        
        <AllowSynchronousIO>true</AllowSynchronousIO>
        <DisableStringReuse>false</DisableStringReuse>
      </Kestrel>
      
      <IISExpress>
        <Enabled>true</Enabled>
        <Port>44300</Port>
        <SSLPort>44301</SSLPort>
        <AnonymousAuthentication>true</AnonymousAuthentication>
        <WindowsAuthentication>false</WindowsAuthentication>
        <EnvironmentVariables>
          <Variable Name="ASPNETCORE_ENVIRONMENT" Value="Development" />
        </EnvironmentVariables>
      </IISExpress>
    </Hosting>
    
    <Debugging>
      <Enabled>true</Enabled>
      <DebuggerType>Mixed</DebuggerType>
      <SymbolPaths>
        <Path>C:\Projects\NEDA\Symbols</Path>
        <Path>https://msdl.microsoft.com/download/symbols</Path>
      </SymbolPaths>
      <JustMyCode>false</JustMyCode>
      <EnableDiagnostics>true</EnableDiagnostics>
      <ExceptionSettings>
        <BreakOnFirstChance>false</BreakOnFirstChance>
        <BreakOnUnhandled>true</BreakOnUnhandled>
        <Exceptions>
          <Exception Type="System.NullReferenceException" Break="true" />
          <Exception Type="System.ArgumentException" Break="true" />
          <Exception Type="System.InvalidOperationException" Break="true" />
        </Exceptions>
      </ExceptionSettings>
    </Debugging>
  </RuntimeConfiguration>

  <DatabaseConfiguration>
    <Providers>
      <SqlServer>
        <Enabled>true</Enabled>
        <ConnectionString>Server=(localdb)\mssqllocaldb;Database=NedaDevelopment;Trusted_Connection=True;MultipleActiveResultSets=true;Connect Timeout=30;Application Name=NEDA Dev</ConnectionString>
        <CommandTimeout>30</CommandTimeout>
        <EnableRetryOnFailure>true</EnableRetryOnFailure>
        <MaxRetryCount>3</MaxRetryCount>
        <MaxRetryDelay>5</MaxRetryDelay>
        <Pooling Enabled="true" MinPoolSize="5" MaxPoolSize="100" />
      </SqlServer>
      
      <Sqlite>
        <Enabled>true</Enabled>
        <ConnectionString>Data Source=C:\Projects\NEDA\Data\NedaDev.db</ConnectionString>
        <CacheMode>Default</CacheMode>
        <ForeignKeys>true</ForeignKeys>
        <RecursiveTriggers>true</RecursiveTriggers>
      </Sqlite>
      
      <PostgreSQL>
        <Enabled>false</Enabled>
        <ConnectionString>Host=localhost;Database=nedadev;Username=postgres;Password=devpassword</ConnectionString>
      </PostgreSQL>
      
      <MongoDB>
        <Enabled>true</Enabled>
        <ConnectionString>mongodb://localhost:27017</ConnectionString>
        <DatabaseName>NedaDev</DatabaseName>
        <UseSSL>false</UseSSL>
        <ConnectionTimeout>30</ConnectionTimeout>
        <ServerSelectionTimeout>30</ServerSelectionTimeout>
      </MongoDB>
    </Providers>
    
    <MigrationSettings>
      <AutoMigrate>true</AutoMigrate>
      <EnableMigrations>true</EnableMigrations>
      <MigrationsAssembly>NEDA.Core</MigrationsAssembly>
      <MigrationsNamespace>NEDA.Core.Migrations</MigrationsNamespace>
      <ContextType>NedaDevelopmentDbContext</ContextType>
    </MigrationSettings>
    
    <SeedData>
      <Enabled>true</Enabled>
      <DataFiles>
        <File Path="C:\Projects\NEDA\Data\Seed\Users.json" />
        <File Path="C:\Projects\NEDA\Data\Seed\Projects.json" />
        <File Path="C:\Projects\NEDA\Data\Seed\Templates.json" />
      </DataFiles>
      <OverwriteExisting>true</OverwriteExisting>
      <PreserveRelationships>true</PreserveRelationships>
    </SeedData>
    
    <Caching>
      <MemoryCache Enabled="true" SizeLimit="100" />
      <Redis Enabled="true">
        <ConnectionString>localhost:6379,abortConnect=false,connectTimeout=3000</ConnectionString>
        <InstanceName>NedaDev:</InstanceName>
        <DefaultSlidingExpiration>300</DefaultSlidingExpiration>
      </Redis>
      <DistributedCache Enabled="true" Provider="Redis" />
    </Caching>
  </DatabaseConfiguration>

  <SecurityConfiguration>
    <Authentication>
      <Jwt>
        <Issuer>NEDA-Development</Issuer>
        <Audience>NEDA-Client-Dev</Audience>
        <SecretKey>DevelopmentSecretKey_ChangeInProduction_32BytesLong!!</SecretKey>
        <TokenExpiration>3600</TokenExpiration>
        <RefreshTokenExpiration>86400</RefreshTokenExpiration>
        <ValidateIssuer>true</ValidateIssuer>
        <ValidateAudience>true</ValidateAudience>
        <ValidateLifetime>true</ValidateLifetime>
        <RequireHttpsMetadata>false</RequireHttpsMetadata>
        <ClockSkew>5</ClockSkew>
      </Jwt>
      
      <ApiKeys>
        <Enabled>true</Enabled>
        <Keys>
          <Key Name="DevelopmentClient" Value="dev-api-key-12345-67890" Permissions="Read,Write,Execute" />
          <Key Name="TestRunner" Value="test-api-key-98765-43210" Permissions="Read,Execute" />
          <Key Name="SwaggerUI" Value="swagger-dev-key-abcde-fghij" Permissions="Read" />
        </Keys>
      </ApiKeys>
      
      <WindowsAuthentication>
        <Enabled>false</Enabled>
      </WindowsAuthentication>
    </Authentication>
    
    <Authorization>
      <Policies>
        <Policy Name="DeveloperPolicy" Requirements="RequireAuthenticatedUser,RequireRole:Developer" />
        <Policy Name="AdminPolicy" Requirements="RequireAuthenticatedUser,RequireRole:Administrator" />
        <Policy Name="ApiPolicy" Requirements="RequireApiKey" />
      </Policies>
      
      <Roles>
        <Role Name="Developer" Permissions="Read,Write,Execute,Debug" />
        <Role Name="Administrator" Permissions="Read,Write,Execute,Delete,Admin" />
        <Role Name="Tester" Permissions="Read,Execute" />
        <Role Name="Viewer" Permissions="Read" />
      </Roles>
    </Authorization>
    
    <Encryption>
      <Algorithm>AES-256-CBC</Algorithm>
      <Key>DevEncryptionKey32BytesLongForAES256!!</Key>
      <IV>DevInitializationVector16B</IV>
      <KeyDerivationIterations>10000</KeyDerivationIterations>
      <KeySize>256</KeySize>
      <BlockSize>128</BlockSize>
    </Encryption>
    
    <DevelopmentSecurity>
      <BypassAuthentication>false</BypassAuthentication>
      <BypassAuthorization>false</BypassAuthorization>
      <AllowHttp>true</AllowHttp>
      <DisableHttpsRedirection>false</DisableHttpsRedirection>
      <DisableSecurityHeaders>false</DisableSecurityHeaders>
      <ShowDetailedErrors>true</ShowDetailedErrors>
      <EnableDeveloperExceptionPage>true</EnableDeveloperExceptionPage>
    </DevelopmentSecurity>
  </SecurityConfiguration>

  <LoggingConfiguration>
    <LogLevel Default="Debug">
      <Override Category="Microsoft" Level="Information" />
      <Override Category="Microsoft.EntityFrameworkCore" Level="Debug" />
      <Override Category="Microsoft.EntityFrameworkCore.Database.Command" Level="Information" />
      <Override Category="System" Level="Information" />
      <Override Category="NEDA" Level="Debug" />
    </LogLevel>
    
    <Providers>
      <Console>
        <Enabled>true</Enabled>
        <Formatter>Simple</Formatter>
        <IncludeScopes>true</IncludeScopes>
        <TimestampFormat>yyyy-MM-dd HH:mm:ss.fff</TimestampFormat>
        <UseUtcTimestamp>false</UseUtcTimestamp>
        <SingleLine>true</SingleLine>
        <Colors>
          <Default>White</Default>
          <Information>Cyan</Information>
          <Warning>Yellow</Warning>
          <Error>Red</Error>
          <Critical>DarkRed</Critical>
          <Debug>Gray</Debug>
        </Colors>
      </Console>
      
      <File>
        <Enabled>true</Enabled>
        <Path>C:\Projects\NEDA\Logs\NEDA-{Date}.log</Path>
        <Format>JSON</Format>
        <FileSizeLimit>10485760</FileSizeLimit>
        <RetainedFileCountLimit>30</RetainedFileCountLimit>
        <RollingInterval>Day</RollingInterval>
        <IncludeScopes>true</IncludeScopes>
        <TimestampFormat>yyyy-MM-dd HH:mm:ss.fff</TimestampFormat>
      </File>
      
      <Debug>
        <Enabled>true</Enabled>
      </Debug>
      
      <EventLog>
        <Enabled>false</Enabled>
        <SourceName>NEDA Development</SourceName>
        <LogName>Application</LogName>
      </EventLog>
      
      <ApplicationInsights>
        <Enabled>true</Enabled>
        <InstrumentationKey>dev-instrumentation-key</InstrumentationKey>
        <EnableAdaptiveSampling>false</EnableAdaptiveSampling>
        <EnablePerformanceCounterCollection>true</EnablePerformanceCounterCollection>
        <EnableDependencyTracking>true</EnableDependencyTracking>
        <EnableHeartbeat>true</EnableHeartbeat>
        <SamplingPercentage>100.0</SamplingPercentage>
      </ApplicationInsights>
      
      <Seq>
        <Enabled>true</Enabled>
        <ServerUrl>http://localhost:5341</ServerUrl>
        <ApiKey>dev-seq-api-key</ApiKey>
        <DefaultLevel>Debug</DefaultLevel>
      </Seq>
    </Providers>
    
    <Filters>
      <Filter Category="Microsoft.EntityFrameworkCore.*" MinLevel="Warning" />
      <Filter Category="System.Net.Http.*" MinLevel="Information" />
    </Filters>
    
    <Enrichers>
      <Enricher Type="ThreadIdEnricher" />
      <Enricher Type="MachineNameEnricher" />
      <Enricher Type="EnvironmentUserNameEnricher" />
      <Enricher Type="AssemblyVersionEnricher" />
    </Enrichers>
  </LoggingConfiguration>

  <MonitoringConfiguration>
    <HealthChecks>
      <Enabled>true</Enabled>
      <Endpoint>/health</Endpoint>
      <UIEndpoint>/health-ui</UIEndpoint>
      <EvaluationTimeInSeconds>10</EvaluationTimeInSeconds>
      <MinimumSecondsBetweenFailureNotifications>60</MinimumSecondsBetweenFailureNotifications>
      
      <Checks>
        <Check Name="Database" Type="SqlServer" Enabled="true">
          <ConnectionString>Server=(localdb)\mssqllocaldb;Database=NedaDevelopment;Trusted_Connection=True</ConnectionString>
        </Check>
        <Check Name="Redis" Type="Redis" Enabled="true">
          <ConnectionString>localhost:6379</ConnectionString>
        </Check>
        <Check Name="ExternalAPI" Type="Http" Enabled="true">
          <Uri>https://api.nuget.org/v3/index.json</Uri>
        </Check>
        <Check Name="DiskStorage" Type="DiskStorage" Enabled="true">
          <DriveName>C:</DriveName>
          <MinimumFreeMegabytes>1024</MinimumFreeMegabytes>
        </Check>
        <Check Name="Memory" Type="Memory" Enabled="true">
          <MaximumMemoryBytes>1073741824</MaximumMemoryBytes>
        </Check>
      </Checks>
    </HealthChecks>
    
    <Metrics>
      <Enabled>true</Enabled>
      <Endpoint>/metrics</Endpoint>
      <CollectIntervalSeconds>15</CollectIntervalSeconds>
      
      <Counters>
        <Counter Name="cpu_usage" Description="CPU usage percentage" Type="Gauge" />
        <Counter Name="memory_usage" Description="Memory usage in bytes" Type="Gauge" />
        <Counter Name="request_duration_seconds" Description="HTTP request duration" Type="Histogram" />
        <Counter Name="error_rate" Description="Error rate per minute" Type="Counter" />
        <Counter Name="active_connections" Description="Active database connections" Type="Gauge" />
        <Counter Name="build_duration_seconds" Description="Build duration in seconds" Type="Histogram" />
      </Counters>
    </Metrics>
    
    <PerformanceCounters>
      <Enabled>true</Enabled>
      <Counters>
        <Counter Category="Processor" Counter="% Processor Time" Instance="_Total" />
        <Counter Category="Memory" Counter="Available MBytes" />
        <Counter Category="Process" Counter="Private Bytes" Instance="NEDA.API" />
        <Counter Category=".NET CLR Memory" Counter="# Bytes in all Heaps" Instance="NEDA.API" />
        <Counter Category="ASP.NET Applications" Counter="Requests/Sec" Instance="NEDA.API" />
      </Counters>
      <SampleInterval>5</SampleInterval>
    </PerformanceCounters>
    
    <Alerting>
      <Enabled>true</Enabled>
      <Channels>
        <Channel Type="Console" />
        <Channel Type="File" Path="C:\Projects\NEDA\Logs\Alerts.log" />
        <Channel Type="Email" Enabled="false" />
      </Channels>
      <Thresholds>
        <Threshold Metric="cpu_usage" Warning="80" Critical="95" />
        <Threshold Metric="memory_usage" Warning="85" Critical="95" />
        <Threshold Metric="error_rate" Warning="5" Critical="10" />
      </Thresholds>
    </Alerting>
  </MonitoringConfiguration>

